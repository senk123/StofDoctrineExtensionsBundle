{
    "name": "agf/doctrine-extensions-bundle",
    "type": "symfony-bundle",
    "description": "Integration of the gedmo/doctrine-extensions with Symfony2",
    "keywords": ["tree", "behaviors", "doctrine2", "extensions", "gedmo", "sluggable","loggable", "translatable", "nestedset", "sortable", "timestampable"],
    "homepage": "https://github.com/stof/StofDoctrineExtensionsBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Coevoet",
            "email": "stof@notk.org"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/senk123/DoctrineExtensions.git"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~5.0",
        "agf/doctrine-extensions": "^2.3.4"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.0",
        "symfony/security-bundle": "^5.0"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "to use the ORM extensions",
        "doctrine/mongodb-odm-bundle": "to use the MongoDB ODM extensions"
    },
    "autoload": {
        "psr-4": { "Stof\\DoctrineExtensionsBundle\\": "" }
    }
 
}
